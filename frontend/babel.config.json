{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      { "runtime": "automatic", "importSource": "@emotion/react" }
    ],
    "@emotion/babel-preset-css-prop",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@emotion/babel-plugin"],
    [
      "babel-plugin-direct-import",
      { "modules": ["@mui/material", "@mui/icons-material"] }
    ]
  ]
}
